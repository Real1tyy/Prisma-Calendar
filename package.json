{
	"name": "prisma-calendar",
	"version": "1.30.0",
	"description": "Feature-rich, fully configurable calendar plugin for Obsidian with multiple calendars, templater, color rules, batch ops, and recurring events.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"test": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -f main.js",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"check": "pnpm run format && pnpm run lint",
		"check:fix": "pnpm run format:fix && pnpm run lint:fix"
	},
	"keywords": [
		"obsidian",
		"calendar",
		"plugin",
		"scheduling",
		"events",
		"recurring"
	],
	"funding": {
		"type": "custom",
		"url": "https://matejvavroproductivity.com/support/"
	},
	"author": "Real1tyy",
	"license": "MIT",
	"packageManager": "pnpm@9.0.0",
	"pnpm": {
		"overrides": {
			"obsidian": "1.8.7"
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/luxon": "^3.7.1",
		"@types/node": "^24.10.9",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.11",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.14.0",
		"jsdom": "^27.4.0",
		"knip": "^5.81.0",
		"obsidian": "1.8.7",
		"prettier": "^3.4.2",
		"tslib": "2.8.1",
		"typescript": "5.9.3",
		"typescript-eslint": "^8.20.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@fullcalendar/core": "^6.1.20",
		"@fullcalendar/daygrid": "^6.1.20",
		"@fullcalendar/interaction": "^6.1.20",
		"@fullcalendar/list": "^6.1.20",
		"@fullcalendar/timegrid": "^6.1.20",
		"@real1ty-obsidian-plugins": "file:./shared",
		"chart.js": "^4.5.1",
		"colord": "^2.9.3",
		"ical.js": "^2.2.1",
		"luxon": "^3.7.2",
		"nanoid": "^5.1.6",
		"rxjs": "^7.8.2",
		"sorted-btree": "^2.1.0",
		"tsdav": "^2.1.6",
		"uuid": "^13.0.0",
		"yaml": "^2.8.2",
		"zod": "^4.3.5"
	}
}
