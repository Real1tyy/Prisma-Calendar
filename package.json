{
	"name": "prisma-calendar",
	"version": "1.8.0",
	"description": "Feature-rich, fully configurable calendar plugin for Obsidian with multiple calendars, templater, color rules, batch ops, and recurring events.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"test": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -f main.js",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"lint:eslint": "eslint src/",
		"lint:eslint:fix": "eslint --fix src/",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"check": "biome check .",
		"check:fix": "biome check --write ."
	},
	"keywords": [
		"obsidian",
		"calendar",
		"plugin",
		"scheduling",
		"events",
		"recurring"
	],
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/Real1tyy"
	},
	"author": "Real1tyy",
	"license": "MIT",
	"packageManager": "pnpm@9.0.0",
	"pnpm": {
		"overrides": {
			"obsidian": "1.8.7"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@types/luxon": "^3.7.1",
		"@types/node": "^24.10.1",
		"@typescript-eslint/parser": "^8.48.0",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.11",
		"eslint": "^9.39.1",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"jsdom": "^27.2.0",
		"obsidian": "1.8.7",
		"tslib": "2.8.1",
		"typescript": "5.9.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@fullcalendar/core": "^6.1.19",
		"@fullcalendar/daygrid": "^6.1.19",
		"@fullcalendar/interaction": "^6.1.19",
		"@fullcalendar/list": "^6.1.19",
		"@fullcalendar/timegrid": "^6.1.19",
		"@real1ty-obsidian-plugins/common-plugin": "^1.3.2",
		"@real1ty-obsidian-plugins/utils": "^2.12.0",
		"chart.js": "^4.5.1",
		"chrono-node": "^2.9.0",
		"colord": "^2.9.3",
		"ical.js": "^2.2.1",
		"luxon": "^3.7.2",
		"nanoid": "^5.1.6",
		"rrule": "^2.8.1",
		"rxjs": "^7.8.2",
		"tsdav": "^2.1.6",
		"uuid": "^13.0.0",
		"zod": "^4.1.12"
	}
}
